{
  "name": "illmatix/modthree-library",
  "description": "A PHP library for the Modulo Three FSM challenge",
  "type": "library",
  "authors": [
    {
      "name": "Chad Payne",
      "email": "chadpayne@gmail.com"
    }
  ],
  "version": "1.0.0",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/illmatix/modthree-challenge"
    }
  ],
  "require": {
    "illmatix/modthree": "dev-main"
  },
  "scripts": {
    "test": "phpunit"
  },
  "require": {
    "php": ">=8.4"
  },
  "autoload": {
    "psr-4": {
      "illmatix\\modThree\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "illmatix\\modThree\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  }
}
